number = {4},
pages = {779--809},
title = {{Science Approach Making Sense of Technology Trends in the Information Technology Landscape : A Design Science Approach1}},
url = {http://www.jstor.org/stable/25148872 http://about.jstor.org/terms},
volume = {32},
