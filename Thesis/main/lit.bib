title = {{Evaluation of the professional worth of scientific papers, their citation responding and the publication authority Journal of Thermal Analysis and Calorimetry}},
url = {https://link.springer.com/content/pdf/10.1007{\%}2Fs10973-017-6178-7.pdf}
