pages = {779--809},
title = {{MAKING SENSE OF TECHNOLOGY TRENDS IN THE INFORMATION TECHNOLOGY LANDSCAPE: A DESIGN SCIENCE APPROACH}},
url = {http://content.ebscohost.com/ContentServer.asp?T=P{\&}P=AN{\&}K=34960502{\&}S=R{\&}D=buh{\&}EbscoContent=dGJyMNHX8kSeprE4xNvgOLCmr1Cep7BSrqq4SrCWxWXS{\&}ContentCustomer=dGJyMOzprkmxr7dLuePfgeyx44Dt6fIA},
